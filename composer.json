{
    "name": "akhilduggirala/laravel-log-enhancer",
    "description": "Enhanced logging helpers for Laravel (smart message/context normalization).",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Kks",
            "email": "keerthanasaisuki@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "illuminate/support": "^9.0 || ^10.0 || ^11.0 || ^12.0",
        "orchestra/testbench": "^9.15"
    },
    "autoload": {
        "psr-4": {
            "Kks\\LogEnhancer\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kks\\LogEnhancer\\LogEnhancerServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "^11.5",
        "orchestra/testbench": "^9.15"
    }
}
