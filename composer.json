{
    "name": "kks/log-enhancer",
    "description": "Enhanced info() logging functionality for Laravel.",
    "type": "library",
    "keywords": ["laravel", "logging", "logger", "info"],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Kks\\LogEnhancer\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kks\\LogEnhancer\\LogEnhancerServiceProvider"
            ]
        }
    },
    "require": {
        "php": ">=8.1",
        "illuminate/support": "^12.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^3.6"
    }
}
